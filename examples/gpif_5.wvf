; waveform source file for gpif_compiler
;
; Comment header
;
	.WAVEFORM 	5		; Name this waveform

	.TRICTL		1		; Assume TRICTL=1

	.IFCLKSRC	1		; feed internal 30/48 MHz to the GPIF
	.3048MHZ	0		; 30 MHz
	.IFCLKOE	0		; IFCLK tri-state, CTL2 drives the ADC

	D	3			OE2		; 3 cycle, CTL2 low
	Z	2			CTL2 OE2	; 2 cycle, CTL2 high
	J 	RDY0 AND RDY0 $0 $0	CTL2 OE2	; 1 cycle, CTL2 high, jp 0

; End
