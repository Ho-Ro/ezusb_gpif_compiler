; waveform source file for gpif_compiler
;
; Comment header
;
	.WAVEFORM	24		; Name this waveform

	.TRICTL		1		; Assume TRICTL=1

	.IFCLKSRC	1		; feed internal 30/48 MHz to the GPIF
	.3048MHZ	1		; 48 MHz
	.IFCLKOE	0		; IFCLK tri-state, CTL2 drives the ADC

	D	1			OE2		; 1 cycles, CTL2 low
	J 	RDY0 AND RDY0 $0 $0	CTL2 OE2	; 1 cycle, jp 0

; End
