; Test waveform file for ezusbcc.cpp
;
; Comment header
;
	.TRICTL		1		; Assume TRICTL=1
	.EP		6		; Assume for Endpoint 6

	.WAVEFORM 	1		; Name this waveform

	.IFCLKSRC	1		; feed internal 30/48 MHz to the GPIF
	.3048MHZ	1		; 48 MHz
	.IFCLKOE	0		; IFCLK tri-state, CTL2 drives the ADC

	D	1			OE2		; 1 cycle, CTL2 low
	Z	1			CTL2 OE2	; 1 cycle, CTL2 high
	J 	RDY0 AND RDY0 $0 $0	CTL2 OE2	; 1 cycle, CTL2 high, jp 0

; End
